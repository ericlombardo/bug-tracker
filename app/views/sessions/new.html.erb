<%= form_with url: login_path do |f| %>  <!-- show errors for user instance if present --> 

  <%= f.label :email %> <!-- display user input fields for login -->
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br>
  <%= f.submit "Login" %>
<% end %>
