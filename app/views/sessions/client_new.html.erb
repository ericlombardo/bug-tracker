<h1>Login To Report A Bug</h1>
<%= form_with url: client_login_path  do |f| %>  
  <%= f.label :email %> 
  <%= f.email_field :email, {required: true} %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Login" %>
<% end %>
<p>First time reporting a bug? <a href="<%= client_signup_path %>">Signup Here</a></p>
<%= button_to 'Google', '/auth/google_oauth2' %>