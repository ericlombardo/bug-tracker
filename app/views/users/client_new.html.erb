<% content_for :login do %>
  <div class="container">
    <div class=" col-6 mx-auto">
      <div id="login_box" class="row rounded button" style="background-color:rgba(0, 0, 0, 0.3);">
        <h3 class="text-center">Client Signup</h3>
        <div class="text-center">
          <%= render 'signup_form' %>
          <%= render 'users/google_oauth' %>
        </div>
      </div>
    </div>
  </div> 
<% end %>
